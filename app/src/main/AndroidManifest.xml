<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yooo.ultimatechat">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Sign in"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" android:label="Register" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".ChatListActivity" android:label="ChatList" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".ChatListObject" android:label="ChatListObject" android:parentActivityName=".ChatListActivity"></activity>

    </application>

</manifest>